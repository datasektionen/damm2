{
  "trailingComma": "all",
  "singleQuote": true,
  "endOfLine": "auto",
  "importOrder": [
    "dotenv/config",
    "^react$",
    "<THIRD_PARTY_MODULES>",
    "^@/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderParserPlugins": [
    "typescript",
    "jsx",
    "decorators-legacy"
  ]
}