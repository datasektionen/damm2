{
    "compilerOptions": {
      "target": "es2017",
      "allowJs": false,
      "module": "commonjs",
      "outDir": "./dist",
      "strict": true,
      "baseUrl": "./",
      "paths": {
        "*":  ["node_modules/*"]
      },
      "types": [
          "node"
      ],
      // "typeRoots": [
      //   "./node_modules/@types"
      // ],
      "esModuleInterop": true,
      "inlineSourceMap": true
    },
    "exclude": [
      "node_modules",
      "dist",
      "client"
    ]
  }